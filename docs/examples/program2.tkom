int fib(int n) {
    if (n == 1) {
        return 1;
    }

    if (n == 2) {
        return 1;
    }

    return fib(n-1) + fib(n-2);
}

void main() {
    int x = 1;
    int fibVal;

    while (x <= 10) {
        fibVal = fib(x);
        print("Fib(" + x as string + ") = " + fibVal as string);
        x = x + 1;
    }
}